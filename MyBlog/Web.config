      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="INFO" />
        <levelMax value="ERROR" />
        <!--ALL,DEBUG,INFO,WARN,ERROR,FATAL,OFF-->
      </filter>
      <layout type="log4net.Layout.SimpleLayout" />
    </appender>
    <logger name="Logger">
      <appender-ref ref="Logger" />
    </logger>
    <logger name="Auditor">
      <appender-ref ref="Auditor" />
    </logger>
  </log4net>
